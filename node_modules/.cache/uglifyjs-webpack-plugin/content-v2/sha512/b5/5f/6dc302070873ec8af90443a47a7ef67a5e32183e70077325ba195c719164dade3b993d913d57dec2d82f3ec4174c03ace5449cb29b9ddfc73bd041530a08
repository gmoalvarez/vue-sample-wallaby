: DateRangeOption) {\\n      this.dateRangeManager.optionChanged(option);\\n    },\\n    getData() {\\n      if (this.dataIsIncomplete) { return }\\n\\n      // Lets make sure we have both a company and a subject\\n      if (!this.selectedCompany || !this.dateRange) { return }\\n\\n      this.loading = true;\\n\\n      this.networkClient\\n          .getFirmCompanyReaders(CONSTANTS.currentFirm(), this.selectedCompany, this.dateRange)\\n\\n          .then(readers => {\\n\\n            this.topReaders = readers;\\n            this.loading = false;\\n\\n          });\\n\\n      this.loadingFollowers = true;\\n      this.networkClient\\n          .getFirmFollowers(CONSTANTS.currentFirm(), this.selectedCompany, this.dateRange)\\n          .then( followers => {\\n\\n            this.topFollowers = followers.sort((a,b) => {\\n              return b.dateAdded.valueOf() - a.dateAdded.valueOf();\\n            });\\n            this.loadingFollowers = false;\\n\\n          })\\n    }\\n  },\\n  watch: {\\n    selectedCompany: function(newVal, oldVal) {\\n      this.snsPublisher.publishSearchEvent(this.selectedCompany.id);\\n\\n      this.getData();\\n    },\\n    selectedFirm: function() {\\n      this.getData();\\n    },\\n    id: function() {\\n      if (!this.companies || this.companies.length <= 0 || !this.id) { return }\\n\\n      for (const company of this.companies) {\\n        if (company.id == this.id) {\\n          this.selectedCompany = company;\\n          this.getData();\\n          return\\n        }\\n      }\\n    },\\n    topReaders: function() {\\n      this.articleContainers = this.topReaders;\\n      this.topicDataSet = this.topReaders;\\n    },\\n    dateRange: function() {\\n      this.getData();\\n    },\\n  }\\n});\\n\\n\",\"var render = function() {\\n  var _vm = this\\n  var _h = _vm.$createElement\\n  var _c = _vm._self._c || _h\\n  return _c(\\n    \\\"div\\\",\\n    { staticClass: \\\"animated fadeIn\\\" },\\n    [\\n      _c(\\\"upgrade\\\", {\\n 