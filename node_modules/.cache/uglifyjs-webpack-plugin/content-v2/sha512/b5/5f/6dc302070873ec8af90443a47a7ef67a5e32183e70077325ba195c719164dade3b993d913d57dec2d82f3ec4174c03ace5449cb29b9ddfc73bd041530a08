els/Company';\\nimport {CompanyReportResolver} from './ReportResolver';\\nimport {SubjectDTO} from '../models/Subject';\\n\\n/* Intermediate object used to hold the view counts and items where the key is the id of the item.\\n * They are used to find the top view counts and sort. */\\ninterface ViewCountInfo {\\n\\n  /* Holds the view counts where the key is the id of the item*/\\n  viewCountDict: any\\n\\n  /* Holds the items themselves as the value, and the id of the item as the key */\\n  itemDict: any\\n\\n}\\n\\nexport interface IViewCounter {\\n  getViewCount(collection: (ArticleContaining & ViewCountContaining)[]): ViewCountInfo\\n\\n  sortViewCount(viewCounter: ViewCountInfo): any[][]\\n\\n  getArticlesArray(collection: (ArticleContaining & ViewCountContaining)[]): Article[][]\\n\\n  incrementCount(item: Identifiable, viewCounter: ViewCountInfo, article: ViewCountContaining)\\n\\n}\\n\\nexport abstract class BaseViewCounter {\\n  sortViewCount(viewCountInfo: ViewCountInfo): any[][] {\\n    // 4. Lets turn this into an array so we can sort them. So, this is a two-dimensional\\n    // array with the first element as the key, and the view count as the second element.\\n    const items = Object.keys(viewCountInfo.viewCountDict).map(key => {\\n      return [key, viewCountInfo.viewCountDict[key]];\\n    });\\n\\n    // 5. Ok, now lets sort by the viewcount, which is the second element in each array.\\n    items.sort((a, b) => {\\n      return b[1] - a[1]\\n    });\\n    return items\\n  }\\n\\n  getArticlesArray(collection: (ArticleViewDataSet)[]): Article[][] {\\n    return collection\\n      .map(item => {\\n        return item.articles;\\n      });\\n  }\\n\\n  incrementCount(item: Identifiable, viewCounter: ViewCountInfo, article: ViewCountContaining) {\\n    const id = item.identifier;\\n    if (viewCounter.viewCountDict[`${id}`]) {\\n      viewCounter.viewCountDict[`${id}`] += article.views\\n    } else {\\n      viewCounter.viewCountDict[`${id}`] = article.views;\\n      viewCounter.itemDict[`${id}`] = item;\\n    }\\n  }\\n}\\n\\nexport class TopicViewCounter extends BaseViewCounter implements IViewCounter {\\n\\n  /*Use either an array of Readers, or simply an array of articles that also contains the total views.*/\\n  getViewCount(collection: (ArticleContaining & ViewCountContaining)[]): ViewCountInfo {\\n    const viewCounter: ViewCountInfo = {viewCountDict: {}, itemDict: {}};\\n\\n    // 2. Lets get the two-dimensional array of articles from the readers\\n    const articlesArray = this.getArticlesArray(collection);\\n\\n    // 3. Now lets iterate over those articles and populate the viewCount dictionary\\n    // We end up with a dictionary that holds the guids of the articles as the key, and\\n    // the view counts as the value\\n    for (const articles of articlesArray) {\\n      for (const article of articles) {\\n        for (const topic of article.topics) {\\n          this.incrementCount(topic, viewCounter, article);\\n        }\\n      }\\n    }\\n    return viewCounter;\\n  }\\n\\n}\\n\\nexport class SubjectViewCounter extends BaseViewCounter implements IViewCounter {\\n\\n  getViewCount(readers: IReader[]): ViewCountInfo {\\n    return this.getViewCountInfo(readers);\\n  }\\n\\n  getViewCountFromDTO(readers: ReaderDTO[]): ViewCountInfo {\\n    return this.getViewCountInfo(readers);\\n  }\\n\\n  private getViewCountInfo(readers: ReaderDTO[] | IReader[]): ViewCountInfo {\\n    const viewCounter: ViewCountInfo = {viewCountDict: {}, itemDict: {}};\\n\\n    // 2. Lets get the two-dimensional array of articles from the readers\\n    const articlesArray = this.getArticlesArrayInternal(readers);\\n\\n    // 3. Now lets iterate over those articles and populate the viewCount dictionary\\n    // We end up with a dictionary that holds the guids of the articles as the key, and\\n    // the view counts as the value\\n    for (const articles of articlesArray) {\\n      for (const article of articles) {\\n        for (const subject of article.subjects) {\\n          this.incrementCount(subject, viewCounter, article);\\n        }\\n      }\\n    }\\n\\n    return viewCounter;\\n  }\\n\\n  private getArticlesArrayInternal(readers: ReaderDTO[] | IReader[]): any[][] {\\n    const articles: any[][] = [];\\n    for (const reader of readers) {\\n      articles.push(reader.articles);\\n    }\\n    return articles;\\n  }\\n\\n  incrementCount(item: Identifiable, viewCounter: ViewCountInfo, article: ViewCountContaining) {\\n    const id = item.identifier;\\n    if (viewCounter.viewCountDict[`${id}`]) {\\n      viewCounter.viewCountDict[`${id}`] += article.views\\n    } else {\\n      viewCounter.viewCountDict[`${id}`] = article.views;\\n      viewCounter.itemDict[`${id}`] = item;\\n    }\\n  }\\n}\\n\\nexport class AuthorViewCounter extends BaseViewCounter implements IViewCounter {\\n\\n  getViewCount(readers: IReader[]): ViewCountInfo {\\n    const viewCounter: ViewCountInfo = {viewCountDict: {}, itemDict: {}};\\n\\n    // 2. Lets get the two-dimensional array of articles from the readers\\n    const articlesArray = this.getArticlesArray(readers);\\n\\n    // 3. Now lets iterate over those articles and populate the viewCount dictionary\\n    // We end up with a dictionary that holds the guids of the articles as the key, and\\n    // the view counts as the value\\n    for (const articles of articlesArray) {\\n      for (const article of articles) {\\n        for (const author of article.authors) {\\n          this.incrementCount(author, viewCounter, article);\\n        }\\n      }\\n    }\\n\\n    return viewCounter;\\n  }\\n}\\n\\nexport class ArticleViewCounter extends BaseViewCounter implements IViewCounter {\\n\\n  getViewCount(readers: IReader[]): ViewCountInfo {\\n    const viewCounter: ViewCountInfo = {viewCountDict: {}, itemDict: {}};\\n\\n    // 2. Lets get the two-dimensional array of articles from the readers\\n    const articlesArray = this.getArticlesArray(readers);\\n\\n    // 3. Now lets iterate over those articles and populate the viewCount dictionary\\n    // We end up with a dictionary that holds the guids of the articles as the key, and\\n    // the view counts as the value\\n    for (const articles of articlesArray) {\\n      for (const article of articles) {\\n        this.incrementCount(article, viewCounter, article);\\n      }\\n    }\\n\\n    return viewCounter;\\n  }\\n}\\n\\nexport class IndustryViewCounter extends BaseViewCounter implements IViewCounter {\\n\\n  getViewCount(readers: IReader[]): ViewCountInfo {\\n    const viewCounter: ViewCountInfo = {viewCountDict: {}, itemDict: {}};\\n\\n    for (const reader of readers) {\\n      for (const industry of reader.industries) {\\n        const id = industry.identifier;\\n        if (viewCounter.viewCountDict[`${id}`]) {\\n          viewCounter.viewCountDict[`${id}`] += reader.views\\n        } else {\\n          viewCounter.viewCountDict[`${id}`] = reader.views;\\n          viewCounter.itemDict[`${id}`] = industry;\\n        }\\n      }\\n    }\\n\\n\\n    return viewCounter;\\n  }\\n}\\n\\nexport class CompanyViewCounter extends BaseViewCounter implements IViewCounter {\\n\\n  getViewCount(readers: IReader[]): ViewCountInfo {\\n    return this.getViewCountInfo(readers);\\n  }\\n\\n  getViewCountFromDTO(readers: ReaderDTO[]): ViewCountInfo {\\n    return this.getViewCountInfo(readers);\\n  }\\n\\n  private getViewCountInfo(readers: ReaderDTO[] | IReader[]): ViewCountInfo {\\n    const viewCounter: ViewCountInfo = {viewCountDict: {}, itemDict: {}};\\n\\n    for (const reader of readers) {\\n      if (!reader.company) { continue }\\n\\n      const id = reader.company;\\n      if (viewCounter.viewCountDict[`${id}`]) {\\n        viewCounter.viewCountDict[`${id}`] += reader.views\\n      } else {\\n        viewCounter.viewCountDict[`${id}`] = reader.views;\\n        viewCounter.itemDict[`${id}`] = new Company(reader.company, 0, '');\\n      }\\n    }\\n\\n    return viewCounter;\\