    topicItemViewFactory: new TopicItemViewFactory(),\\n        topicViewCounter: new TopicViewCounter(),\\n        networkClient: new NetworkClient(),\\n        dateRangeManager: new DateRangeUtil(),\\n        snsPublisher: new SNSMessagePublisher(),\\n        currentUser: new CurrentUser(),\\n\\n        //Data for Mixins\\n        articleContainers: <ArticleContaining[]>[],\\n        topicDataSet: <ArticleViewDataSet[]>[]\\n      }\\n    },\\n\\n    created() {\\n\\n      this.loadingSubjects = true;\\n      this.loadingArticles = true;\\n      this.loadSubjectReports()\\n\\n    },\\n    watch: {\\n\\n      selectedSubject: function(newVal, oldVal) {\\n        this.snsPublisher.publishSearchEvent(this.selectedSubject.id);\\n\\n        this.getData();\\n\\n      },\\n\\n      selectedFirm: function() {\\n\\n        this.getData();\\n\\n      },\\n\\n      dateRange: function() {\\n\\n        this.getData();\\n\\n      },\\n\\n      id: function() {\\n        if (!this.subjects || this.subjects.length <= 0 || !this.id) { return }\\n\\n        this.setSelectedSubjectFromId();\\n\\n      }\\n    },\\n    computed: {\\n\\n      dateRange(): DateRange {\\n        return this.dateRangeManager.dateRange\\n      },\\n\\n      dataIsIncomplete(): boolean {\\n        return this.loadingData ||\\n            !this.selectedSubject ||\\n            !this.selectedSubject.topicIds ||\\n            !this.selectedSubject.subjectIds ||\\n            (this.selectedSubject.topicIds.length <= 0 && this.selectedSubject.subjectIds.length <= 0)\\n      }\\n    },\\n    methods: {\\n\\n      setSelectedSubjectFromId() {\\n        for (const subject of this.subjects) {\\n\\n          // this.id from route is a string, while subject.id is a number, so we use == here\\n          if (subject.id == this.id) {\\n\\n            this.selectedSubject = subject;\\n            this.getData();\\n            return\\n\\n          }\\n\\n        }\\n      },\\n\\n      loadSubjectReports() {\\n\\n        this.networkClient.getSubjectReports()\\n            .then(subjects => {\\n\\n              this.subjects = subjects;\\n              this.loadingSubjects = false;\\n              this.setSelectedSubjectFromId();\\n\\n            });\\n\\n      },\\n\\n      getPGIds(): Promise<PracticeGroupId> {\\n\\n        return this.networkClient.getFirmPracticeGroupIds()\\n\\n            .then( ids => {\\n\\n              this.selectedFirm.id = ids.parent;\\n              this.selectedFirm.practiceGroupIds = ids.children;\\n              return ids;\\n\\n            })\\n      },\\n\\n      getData() {\\n\\n        if (this.dataIsIncomplete) { return }\\n\\n        // Lets make sure we have both a company and a subject\\n        if (!this.selectedFirm || !this.selectedSubject || !this.dateRange) { return }\\n\\n        //Lets make sure we have all of the practice group ids for this firm.\\n        if (this.selectedFirm.id === 0 || this.selectedFirm.allRequestIds.length === 0) {\\n          this.loadingData = true;\\n          this.getPGIds()\\n              .then(() => {\\n                this.loadingData = false;\\n                this.getData();\\n              });\\n          return;\\n        }\\n\\n        this.loadingData = true;\\n\\n        this.networkClient.getFirmSubjectReaders(this.selectedFirm, this.selectedSubject, this.dateRange)\\n\\n            .then(readers => {\\n\\n              this.topReaders = readers;\\n              this.loadingData = false;\\n\\n            });\\n\\n        this.loadingArticles = true;\\n        this.networkClient.getFirmSubjectArticles(this.selectedFirm, this.selectedSubject, this.dateRange)\\n\\n            .then( articleDataSet => {\\n\\n              this.articleContainers = [articleDataSet];\\n              this.topicDataSet = [articleDataSet];\\n              this.loadingArticles = false;\\n\\n            });\\n\\n        this.loadingArticlesPublished = true;\\n        this.networkClient.getFirmOverview(this.selectedFirm, this.selectedSubject, this.dateRange)\\n\\n            .then( overview => {\\n\\n              this.numberOfArticlesPublished = overview.uploads;\\n              this.loadingArticlesPublished = false;\\n\\n            });\\n\\n        this.loadingTweeters = true;\\n        this.networkClient.getFirmTweeters(this.selectedFirm, this.selectedSubject, this.dateRange)\\n\\n            .then( tweeters => {\\n\\n              this.topTweeters = tweeters;\\n              this.loadingTweeters = false;\\n\\n            })\\n      },\\n\\n      onFirmSelected(firm) {\\n        this.selectedFirm = firm;\\n      },\\n\\n      onDateChanged(option: DateRangeOption) {\\n        this.dateRangeManager.optionChanged(option);\\n      }\\n    }\\n  });\\n\\n<\\/script>\\n\\n<style>\\n  .fade-enter-active, .fade-leave-active {\\n    transition: opacity .5s;\\n  }\\n  .fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\\n    opacity: 0;\\n  }\\n</style>\\n'],sourceRoot:\"\"}])},S4u3:function(t,e,n){var s=n(\"fWAt\");\"string\"==typeof s&&(s=[[t.i,s,\"\"]]),s.locals&&(t.exports=s.locals);(0,n(\"SZ7m\").default)(\"0f7a95ae\",s,!1,{})},SxE8:function(t,e,n){var s=n(\"v9fv\");\"string\"==typeof s&&(s=[[t.i,s,\"\"]]),s.locals&&(t.exports=s.locals);(0,n(\"SZ7m\").default)(\"645bd032\",s,!1,{})},T61v:function(t,e,n){var s=n(\"ef9i\");\"string\"==typeof s&&(s=[[t.i,s,\"\"]]),s.locals&&(t.exports=s.locals);(0,n(\"SZ7m\").default)(\"89227ddc\",s,!1,{})},U2up:function(t,e,n){(t.exports=n(\"I1BE\")(!0)).push([t.i,\"\\n<style>\\n@media screen and (min-width: 52em) {\\n.column-count { \\n  -webkit-column-count: 3;\\n  -moz-column-count: 3;\\n  -o-column-count: 3;\\n  column-count: 3;\\n}\\n}\\n\",\"\",{version:3,sources:[\"/Users/guillermo/Development/analytics/www/statistics/src/components/src/components/SubjectList.vue\"],names:[],mappings:\";AA2CA;;AAEA;EACA,wBAAA;EACA,qBAAA;EACA,mBAAA;EACA,gBAAA;CAAA;CACA\",file:\"SubjectList.vue\",sourcesContent:['<template>\\n  <div>\\n    <i v