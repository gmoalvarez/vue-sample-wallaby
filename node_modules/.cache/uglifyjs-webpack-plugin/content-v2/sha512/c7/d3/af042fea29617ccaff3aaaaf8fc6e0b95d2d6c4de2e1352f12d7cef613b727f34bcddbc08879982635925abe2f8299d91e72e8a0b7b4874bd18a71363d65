 TweeterDto2 on the server... ¯\\\\_(ツ)_/¯ */\\nexport interface TweeterDTO {\\n    name: string;\\n    description: string;\\n    tweet_count: number;\\n    tweeter: string;\\n    total_followers: number;\\n    tweets: TweetDTO[];\\n    photo: string;\\n    notable: boolean;\\n    screen_name: string;\\n}\",\"export class FirmOverview {\\n  constructor(\\n    public views: number = 0,\\n    public profiles: number = 0,\\n    public uploads: number = 0\\n  ) {}\\n\\n  static fromJSON(data: FirmOverviewDTO) {\\n    return new this(\\n      data.views,\\n      data.profiles,\\n      data.uploads\\n    )\\n  }\\n}\\n\\nexport interface FirmOverviewDTO {\\n  views: number;\\n  profiles: number;\\n  uploads: number;\\n}\",\"import {SimpleCache} from './SimpleCache';\\nimport {CONSTANTS} from '../entities/Constants';\\nimport {UserInfo, UserInfoDTO} from '../entities/UserInfo';\\n\\nexport class CurrentUser {\\n  constructor(\\n      private userCache = new SimpleCache(CONSTANTS.cacheKey.userInfo)\\n  ) { }\\n\\n  public get userInfo(): UserInfo {\\n    const userData = this.userCache.getData();\\n\\n    if (userData) {\\n      return UserInfo.fromJSON(userData)\\n    }\\n    return new UserInfo('');\\n  }\\n\\n  public addIPAddress(ipAddress: string) {\\n    const currentUser = this.userInfo;\\n    currentUser.ipAddress = ipAddress;\\n    this.userCache.saveData(currentUser)\\n  }\\n\\n  public updateUser(data: UserInfoDTO) {\\n    const userData = this.userCache.getData();\\n\\n    const user = userData ? UserInfo.fromJSON(userData) : new UserInfo('');\\n    this.updateUserProperties(data, user);\\n\\n    this.userCache.saveData(user);\\n  }\\n\\n  private updateUserProperties(data: UserInfoDTO, user: UserInfo) {\\n    if (data.ASPNetUsersId) user.id = data.ASPNetUsersId;\\n    if (data.Title) user.title = data.Title;\\n    if (data.FirmName) user.firmName = data.FirmName;\\n    if (data.LastName) user.lastName = data.LastName;\\n    if (data.FirstName) user.firstName =