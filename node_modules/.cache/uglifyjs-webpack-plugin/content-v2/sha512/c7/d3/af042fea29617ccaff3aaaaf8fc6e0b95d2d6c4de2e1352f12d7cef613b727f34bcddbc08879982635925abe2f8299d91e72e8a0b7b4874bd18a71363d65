               <top-items :loading=loadingFollowers\\\\n                               :items=\\\\\\\"topFollowers\\\\\\\"\\\\n                               :nodata=\\\\\\\"`We don't have information to display here either because we don't have data on your ${selectedCompany.name} followers or because your followers are set to 'private.'`\\\\\\\"\\\\n                    ></top-items>\\\\n                    <a href=\\\\\\\"#\\\\\\\"\\\\n                       @click.prevent=\\\\\\\"openModal\\\\\\\"\\\\n                       class=\\\\\\\"db f7 pt4 tc\\\\\\\"\\\\n                       v-if=\\\\\\\"topFollowers.length > 4\\\\\\\" onclick=\\\\\\\"ga('send', 'event', 'beacon', 'company-report', 'showmore-followers');\\\\\\\">\\\\n                        <i aria-hidden=\\\\\\\"true\\\\\\\" class=\\\\\\\"db bt b--black-05 f7 pv2 tc\\\\\\\"></i>\\\\n                        SHOW MORE+\\\\n                    </a>\\\\n                </div>\\\\n            </div>\\\\n        </section>\\\\n        <!--END TOP READERS -->\\\\n\\\\n        <section class=\\\\\\\"bg-white bb bt b--black-05 pa5-ns ph4 pv5 tc\\\\\\\">\\\\n            <h3 class=\\\\\\\"f3 fw4 mb4 mt0 red\\\\\\\">Follow {{selectedCompany.name}} News & Trends</h3>\\\\n           <!--UPGRADE PROMO-->\\\\n             <div class=\\\\\\\"gray tc\\\\\\\">\\\\n                <i class=\\\\\\\"far fa-lock-alt f4 silver\\\\\\\" aria-hidden=\\\\\\\"true\\\\\\\"></i>\\\\n                <p class=\\\\\\\"center lh-copy mb3 mt2 tc w-40-ns w-30-l\\\\\\\">Stay informed by accessing curated daily news about {{selectedCompany.name}} from the most reputable media sites online.\\\\n                </p>\\\\n                <router-link :to=\\\\\\\"`/company/news/${selectedCompany.id}`\\\\\\\" class=\\\\\\\"bg-transparent btn-outline-animated btn-outline-animated-primary f6 mt1\\\\\\\" \\\\n                onclick=\\\\\\\"ga('send', 'event', 'beacon', 'company-report', 'news-bottom-button');\\\\\\\"><span>Access Industry News</spa