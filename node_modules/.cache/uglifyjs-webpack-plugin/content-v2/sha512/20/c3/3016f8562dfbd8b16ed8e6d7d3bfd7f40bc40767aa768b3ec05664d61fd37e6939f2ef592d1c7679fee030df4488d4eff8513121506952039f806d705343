  .then( articleDataSet => {\\\\n              console.log('Back from getting article data set: ', articleDataSet);\\\\n              this.articleContainers = [articleDataSet];\\\\n              this.topicDataSet = [articleDataSet];\\\\n              this.loadingArticles = false;\\\\n            });\\\\n\\\\n        this.loadingArticlesPublished = true;\\\\n        this.networkClient\\\\n          .getFirmOverview(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n          .then( overview => {\\\\n            console.log('Back from getting firm overview', overview);\\\\n            this.numberOfArticlesPublished = overview.uploads;\\\\n            this.loadingArticlesPublished = false;\\\\n          });\\\\n\\\\n\\\\n        this.loadingTweeters = true;\\\\n        this.networkClient\\\\n            .getFirmTweeters(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n            .then( tweeters => {\\\\n              console.log('Back from getting all tweeters: ', tweeters);\\\\n              this.topTweeters = tweeters;\\\\n              this.loadingTweeters = false;\\\\n            })\\\\n      },\\\\n      onFirmSelected(firm) {\\\\n        this.selectedFirm = firm;\\\\n      },\\\\n      onDateChanged(option: DateRangeOption) {\\\\n        this.dateRangeManager.optionChanged(option);\\\\n      }\\\\n    }\\\\n  });\\\\n\\\\n</script>\\\\n\\\\n<style>\\\\n  .fade-enter-active, .fade-leave-active {\\\\n    transition: opacity .5s;\\\\n  }\\\\n  .fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\\\\n    opacity: 0;\\\\n  }\\\\n</style>\\\\n\\\"],\\\"sourceRoot\\\":\\\"\\\"}]);\\n\\n// exports\\n\",\"// style-loader: Adds some css to the DOM by adding a <style> tag\\n\\n// load the styles\\nvar content = require(\\\"!!../../node_modules/css-loader/index.js?sourceMap!../../node_modules/vue-loader/lib/style-compiler/index.js?{\\\\\\\"optionsId\\\\\\\":\\\\\\\"0\\\\\\\",\\\\\\\"vue\\\\\\\":true,\\\\\\\"scoped\\\\\\\":false,\\