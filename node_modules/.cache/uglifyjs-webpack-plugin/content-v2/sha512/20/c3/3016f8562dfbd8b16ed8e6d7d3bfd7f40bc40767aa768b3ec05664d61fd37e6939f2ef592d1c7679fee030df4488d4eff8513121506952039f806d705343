\n      for (const article of articles) {\\n        const id = article.identifier;\\n        if (viewCount[`${id}`]) {\\n          viewCount[`${id}`] += article.views;\\n        } else {\\n          viewCount[`${id}`] = article.views;\\n          itemDictionary[`${id}`] = article;\\n        }\\n      }\\n    }\\n\\n    // 4. Lets turn this into an array so we can sort them. So, this is a two-dimensional\\n    // array with the first element as the key, and the view count as the second element.\\n    const items = Object.keys(viewCount).map(key => {\\n      return [key, viewCount[key]];\\n    });\\n\\n    // 5. Ok, now lets sort by the viewcount, which is the second element in each array.\\n    console.log('sorting by viewcount: ', items);\\n    items.sort( (a, b) => {\\n      return b[1] - a[1]\\n    });\\n    console.log('after sorting:', items);\\n    // 6. We might only want the top articles. If so, lets slice the array\\n    // The id is in index [0]. The viewcount is in index[1]\\n    let articleIdViewCount: any[][] = [[]];\\n    if (top) {\\n      articleIdViewCount = items.slice(0,top);\\n    }\\n    console.log('top articles: ', articleIdViewCount);\\n    // Done. We can get the articles themselves back from the original dictionary using\\n    // the file guid as the key.\\\\\\n\\n    const articles = articleIdViewCount\\n        .map(idViewCount => {\\n          const article = itemDictionary[`${idViewCount[0]}`];\\n          article.views = idViewCount[1];\\n          return article;\\n        });\\n\\n\\n    console.log('articles: ', articles);\\n    const totalArticleCount = items.length;\\n    console.log('total article count: ', totalArticleCount);\\n    const totalViewCount = items.reduce((acc,cur) => { return acc + cur[1] }, 0);\\n    console.log('totalViewCount: ', totalViewCount);\\n    return this._factory.createSummary(totalViewCount, totalArticleCount, articles)\\n    //return new ArticleSummary(totalVievwCount, totalArticleCount, articles);\\n 