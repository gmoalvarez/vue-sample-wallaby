./entities/Constants';\\nimport {UserInfo, UserInfoDTO} from '../entities/UserInfo';\\n\\nexport class CurrentUser {\\n  constructor(\\n      private userCache = new SimpleCache(CONSTANTS.cacheKey.userInfo)\\n  ) {}\\n\\n  public get userInfo(): UserInfo {\\n    const userData = this.userCache.getData();\\n\\n    if (userData) {\\n      return UserInfo.fromJSON(userData)\\n    }\\n    return new UserInfo('UNKNOWN');\\n  }\\n\\n  public addIPAddress(ipAddress: string) {\\n    const currentUser = this.userInfo;\\n    currentUser.ipAddress = ipAddress;\\n    this.userCache.saveData(currentUser)\\n  }\\n\\n  public updateUser(data: UserInfoDTO) {\\n\\n  }\\n}\",\"import axios, {AxiosInstance, AxiosResponse} from 'axios';\\nimport {jdsUrlStrings} from '../entities/JdsUrlStrings';\\nimport {Subject} from '../models/Subject';\\nimport {Firm} from '../models/Firm';\\nimport {Industry} from '../models/Industry';\\nimport {Tweeter} from '../models/Tweeter';\\nimport {IReader, Reader, ReaderDTO} from '../models/Reader';\\nimport {IURLConstructor, URLConstructor} from './URLConstructor';\\nimport {\\n  CompaniesResponseDTO,\\n  CompanyReportsResponseDTO,\\n  FirmSubjectsResponseDTO,\\n  FollowerResponseDTO,\\n  IndustriesResponseDTO,\\n  IndustryReportsResponseDTO,\\n  SubjectReportsResponseDTO, TweetersResponseDTO\\n} from '../models/ResponseModels';\\nimport {EntitySorter} from './EntitySorter';\\nimport {Company} from '../models/Company';\\nimport {IndustryReport} from '../models/IndustryReport';\\nimport {SubjectReport} from '../models/SubjectReport';\\nimport {CompanyReport} from '../models/CompanyReport';\\nimport {PracticeGroupId} from '../models/PracticeGroupId';\\nimport {Topic} from '../models/Topic';\\nimport {Article} from '../models/Article';\\nimport {DateRange} from '../entities/DateRange';\\nimport {Follower} from '../models/Follower';\\nimport {SimpleCache} from './SimpleCache';\\nimport {UpgradeFormData} from '../models/UpgradeFormData';\\nimport {CONSTANTS} f