SubjectMixin,\\\\n      TopTopicMixin,\\\\n      TopArticleMixin,\\\\n      TopAuthorMixin,\\\\n      TopIndustryMixin,\\\\n      TopTweeterMixin,\\\\n        ModalMixin\\\\n    ],\\\\n    components: {\\\\n      TopItems,\\\\n      NumberSummary,\\\\n      TotalViews,\\\\n      Loading,\\\\n      TopicSummary,\\\\n      SubjectSummary,\\\\n      IndustrySummary,\\\\n      FirmSelect,\\\\n      TopReaders,\\\\n      SubjectHeader,\\\\n      TopTweeters,\\\\n      Upgrade\\\\n    },\\\\n    props: ['id'],\\\\n    data() {\\\\n      return {\\\\n        showDropdown: false,\\\\n        selectedFirm: new Firm('', '', '', 0),\\\\n        subjects: <SubjectReport[]>[],\\\\n        selectedSubject: new SubjectReport('', 0, '', [], []),\\\\n        topReaders: <IReader[]>[],\\\\n        topTweeters: <Tweeter[]>[],\\\\n        loadingData: false,\\\\n        loadingSubjects: false,\\\\n        loadingArticles: false,\\\\n        loadingArticlesPublished: false,\\\\n        loadingTweeters: false,\\\\n        numberOfArticlesPublished: 0,\\\\n        articleSummaryFactory: new ArticleSummaryFactory(),\\\\n        topicItemViewFactory: new TopicItemViewFactory(),\\\\n        topicViewCounter: new TopicViewCounter(),\\\\n        networkClient: new NetworkClient(),\\\\n        dateRangeManager: new DateRangeUtil(),\\\\n\\\\n        //Data for Mixins\\\\n        articleContainers: <ArticleContaining[]>[],\\\\n        topicDataSet: <ArticleViewDataSet[]>[]\\\\n      }\\\\n    },\\\\n    created() {\\\\n\\\\n      this.loadingSubjects = true;\\\\n      this.loadingArticles = true;\\\\n      this.loadSubjectReports()\\\\n\\\\n    },\\\\n    watch: {\\\\n      selectedSubject: function(newVal, oldVal) {\\\\n        console.log('Subject Report watch (selectedSubject): ', newVal);\\\\n        this.getData();\\\\n      },\\\\n      selectedFirm: function() {\\\\n        console.log('selected firm... lets check pg id and get data');\\\\n        console.log(this.selectedFirm);\\\\n        this.getData();\\\\n      },\\\\n      dateRange: function() {\\\\n        this.getData();\\\\n      },\\\\n      id: function() {\\\\n        if (!this.subjects || this.subjects.length <= 0 || !this.id) { return }\\\\n        console.log('id changed:', this.id);\\\\n        this.setSelectedSubjectFromId();\\\\n\\\\n      }\\\\n    },\\\\n    computed: {\\\\n      dateRange(): DateRange {\\\\n        return this.dateRangeManager.dateRange\\\\n      },\\\\n      dataIsIncomplete(): boolean {\\\\n        return this.loadingData ||\\\\n            !this.selectedSubject ||\\\\n            !this.selectedSubject.topicIds ||\\\\n            !this.selectedSubject.subjectIds ||\\\\n            (this.selectedSubject.topicIds.length <= 0 && this.selectedSubject.subjectIds.length <= 0)\\\\n      }\\\\n    },\\\\n    methods: {\\\\n      setSelectedSubjectFromId() {\\\\n        for (const subject of this.subjects) {\\\\n          // this.id from route is a string, while subject.id is a number, so we use == here\\\\n          if (subject.id == this.id) {\\\\n            console.log('Found subject with id: ', this.id);\\\\n            this.selectedSubject = subject;\\\\n            this.getData();\\\\n            return\\\\n          }\\\\n        }\\\\n      },\\\\n      loadSubjectReports() {\\\\n        this.networkClient.getSubjectReports()\\\\n            .then(subjects => {\\\\n              console.log('Got subjects in SubjectReport.vue: ', subjects);\\\\n              console.log('The current id is: ', this.id);\\\\n              this.subjects = subjects;\\\\n              this.loadingSubjects = false;\\\\n              this.setSelectedSubjectFromId();\\\\n            });\\\\n      },\\\\n      getPGIds(): Promise<PracticeGroupId> {\\\\n        return this.networkClient.getFirmPracticeGroupIds()\\\\n            .then( ids => {\\\\n              this.selectedFirm.id = ids.parent;\\\\n              this.selectedFirm.practiceGroupIds = ids.children;\\\\n              return ids;\\\\n            })\\\\n      },\\\\n      getData() {\\\\n        if (this.dataIsIncomplete) {\\\\n          console.log('Missing some data or already loading');\\\\n          console.log([this.selectedFirm, this.loadingData, this.selectedSubject, this.selectedSubject.topicIds, this.selectedSubject.subjectIds]);\\\\n          return\\\\n        }\\\\n\\\\n        console.log('Getting data:');\\\\n        console.log('Selected Firm: ', this.selectedFirm);\\\\n        // Lets make sure we have both a company and a subject\\\\n        if (!this.selectedFirm || !this.selectedSubject || !this.dateRange) { return }\\\\n\\\\n        //Lets make sure we have all of the practice group ids for this firm.\\\\n        if (this.selectedFirm.id === 0 || this.selectedFirm.allRequestIds.length === 0) {\\\\n          this.loadingData = true;\\\\n          this.getPGIds()\\\\n              .then(() => {\\\\n                this.loadingData = false;\\\\n                this.getData();\\\\n              });\\\\n          return;\\\\n        }\\\\n\\\\n        this.loadingData = true;\\\\n\\\\n        this.networkClient\\\\n            .getFirmSubjectReaders(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n            .then(readers => {\\\\n              console.log('Inside Subject Report, the readers are: ', readers);\\\\n              this.topReaders = readers;\\\\n              this.loadingData = false;\\\\n            });\\\\n\\\\n        this.loadingArticles = true;\\\\n        this.networkClient\\\\n            .getFirmSubjectArticles(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n            .then( articleDataSet => {\\\\n              console.log('Back from getting article data set: ', articleDataSet);\\\\n              this.articleContainers = [articleDataSet];\\\\n              this.topicDataSet = [articleDataSet];\\\\n              this.loadingArticles = false;\\\\n            });\\\\n\\\\n        this.loadingArticlesPublished = true;\\\\n        this.networkClient\\\\n          .getFirmOverview(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n          .then( overview => {\\\\n            console.log('Back from getting firm overview', overview);\\\\n            this.numberOfArticlesPublished = overview.uploads;\\\\n            this.loadingArticlesPublished = false;\\\\n          });\\\\n\\\\n\\\\n        this.loadingTweeters = true;\\\\n        this.networkClient\\\\n            .getFirmTweeters(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n            .then( tweeters => {\\\\n              console.log('Back from getting all tweeters: ', tweeters);\\\\n              this.topTweeters = tweeters;\\\\n              this.loadingTweeters = false;\\\\n            })\\\\n      },\\\\n      onFirmSelected(firm) {\\\\n        this.selectedFirm = firm;\\\\n      },\\\\n      onDateChanged(option: DateRangeOption) {\\\\n        this.dateRangeManager.optionChanged(option);\\\\n      }\\\\n    }\\\\n  });\\\\n\\\\n</script>\\\\n\\\\n<style>\\\\n  .fade-enter-active, .fade-leave-active {\\\\n    transition: opacity .5s;\\\\n  }\\\\n  .fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\\\\n    opacity: 0;\\\\n  }\\\\n</style>\\\\n\\\"],\\\"sourceRoot\\\":\\\"\\\"}]);\\n\\n// exports\\n\",\"// 7.1.13 ToObject(argument)\\nvar defined = require('./_defined');\\nmodule.exports = function (it) {\\n  return Object(defined(it));\\n};\\n\",\"// style-loader: Adds some css to the DOM by adding a <style> tag\\n\\n// load the styles\\nvar content = require(\\\"!!../../node_modules/css-loader/index.js?sourceMap!../../node_modules/vue-loader/lib/style-compiler/index.js?{\\\\\\\"optionsId\\\\\\\":\\\\\\\"0\\\\\\\",\\\\\\\"vue\\\\\\\":true,\\\\\\\"scoped\\\\\\\":false,\\\\\\\"sourceMap\\\\\\\":true}!../../node_modules/vue-loader/lib/selector.js?type=styles&index=0!./TotalViews.vue\\\");\\nif(typeof content === 'string') content = [[module.id, content, '']];\\nif(content.locals) module