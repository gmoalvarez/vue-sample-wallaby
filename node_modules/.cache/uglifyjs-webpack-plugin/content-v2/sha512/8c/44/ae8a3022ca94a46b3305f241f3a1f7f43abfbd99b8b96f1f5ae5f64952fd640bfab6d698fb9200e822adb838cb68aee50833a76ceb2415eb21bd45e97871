eters = false;\\n          })\\n    },\\n    onFirmSelected(firm) {\\n      this.selectedFirm = firm;\\n    },\\n    onDateChanged(option: DateRangeOption) {\\n      this.dateRangeManager.optionChanged(option);\\n    }\\n  }\\n});\\n\\n\",\"var render = function() {\\n  var _vm = this\\n  var _h = _vm.$createElement\\n  var _c = _vm._self._c || _h\\n  return _c(\\n    \\\"div\\\",\\n    { staticClass: \\\"animated fadeIn\\\" },\\n    [\\n      _c(\\\"upgrade\\\", {\\n        attrs: {\\n          showModal: _vm.showModal,\\n          source: _vm.selectedSubject.name + \\\" - Subject Insights Tab\\\"\\n        },\\n        on: { onclosed: _vm.closeModal }\\n      }),\\n      _vm._v(\\\" \\\"),\\n      _c(\\\"subject-header\\\", {\\n        attrs: {\\n          selectedSubject: _vm.selectedSubject,\\n          loading: _vm.loadingSubjects\\n        },\\n        on: { datechanged: _vm.onDateChanged }\\n      }),\\n      _vm._v(\\\" \\\"),\\n      _vm._m(0),\\n      _vm._v(\\\" \\\"),\\n      _c(\\\"section\\\", { staticClass: \\\"flex-ns w-100\\\" }, [\\n        _c(\\n          \\\"div\\\",\\n          {\\n            staticClass:\\n              \\\"bg-near-white-ns bg-white br b--black-05 pt2 pt4-ns w-50-ns\\\"\\n          },\\n          [\\n            _c(\\\"div\\\", { staticClass: \\\"mt4 ph3 ph4-ns ph5-l\\\" }, [\\n              _c(\\n                \\\"h1\\\",\\n                { staticClass: \\\"f4-ns f5 fw4 mb2 mb5-ns mt5-ns red tc\\\" },\\n                [\\n                  _vm._v(\\n                    \\\"Your Firm's Visiblity on the Subject of \\\" +\\n                      _vm._s(_vm.selectedSubject.name)\\n                  ),\\n                  _c(\\\"i\\\", {\\n                    directives: [\\n                      {\\n                        name: \\\"tooltip\\\",\\n                        rawName: \\\"v-tooltip.top-center\\\",\\n                        value:\\n                          \\\"How your firm is known for \\\" +\\n                 