\n\\n      return false;\\n\\n    });\\n\\n    return filteredReaders\\n  }\\n\\n  static filterArticlesWithSubjects(articles: Article[], subjectReport: SubjectReport): ArticleViewDataSet {\\n    const subjectIds = subjectReport.subjectIds;\\n    const topicIds = subjectReport.topicIds;\\n\\n    let views = 0;\\n    const filteredArticles = articles.filter(article => {\\n\\n      for (const subject of article.subjects) {\\n\\n        for (const id of subjectIds) {\\n\\n          if (id === subject.id) {\\n            views += article.views;\\n            return true\\n          }\\n\\n        }\\n\\n      }\\n\\n      for (const topic of article.topics) {\\n\\n        for (const id of topicIds) {\\n\\n          if (id === topic.id) {\\n            views += article.views;\\n            return true\\n          }\\n\\n        }\\n\\n      }\\n\\n      return false;\\n\\n    });\\n\\n    return {\\n      articles: filteredArticles,\\n      views: views\\n    }\\n\\n  }\\n\\n  private static readerIsNotable(reader: IReader): boolean {\\n    return reader.notable === true\\n  }\\n\\n  private static readerIsIPReader(reader: IReader): boolean {\\n    return reader.type === ReaderEntityType.registrant;\\n  }\\n\\n  private static readerIsSubscriber(reader: IReader): boolean {\\n    return reader.type === ReaderEntityType.subscriber || reader.type === ReaderEntityType.compscriber;\\n  }\\n\\n  private static removeDuplicateReaders(self, reader, index) {\\n    return self.findIndex(r => {\\n      return this.readerIsDuplicate(r, reader);\\n    }) === index\\n  }\\n\\n  private static readerIsDuplicate(r1: IReader, r2: IReader): boolean {\\n    return r1.displayTitle === r2.displayTitle && r1.company === r2.company && r1.views === r2.views;\\n  }\\n}\",\"import {IReader} from '../models/Reader';\\nimport {DateRangeOption} from '../entities/DateRangeOption';\\n\\nexport class ReaderCache {\\n\\n  private static _instance: ReaderCache;\\n\\n  private constructor() {}\\n\\n  priv