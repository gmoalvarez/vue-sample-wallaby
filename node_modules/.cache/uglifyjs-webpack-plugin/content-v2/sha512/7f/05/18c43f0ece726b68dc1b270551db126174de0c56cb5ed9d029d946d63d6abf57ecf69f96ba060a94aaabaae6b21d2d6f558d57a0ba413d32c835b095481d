om route is a string, while subject.id is a number, so we use == here\\\\n          if (subject.id == this.id) {\\\\n\\\\n            this.selectedSubject = subject;\\\\n            this.getData();\\\\n            return\\\\n\\\\n          }\\\\n\\\\n        }\\\\n      },\\\\n\\\\n      loadSubjectReports() {\\\\n\\\\n        this.networkClient.getSubjectReports()\\\\n            .then(subjects => {\\\\n\\\\n              this.subjects = subjects;\\\\n              this.loadingSubjects = false;\\\\n              this.setSelectedSubjectFromId();\\\\n\\\\n            });\\\\n\\\\n      },\\\\n\\\\n      getData() {\\\\n\\\\n        if (this.dataIsIncomplete) { return }\\\\n\\\\n        // Lets make sure we have both a company and a subject\\\\n        if (!this.selectedSubject || !this.dateRange) { return }\\\\n\\\\n        this.loadingData = true;\\\\n\\\\n        this.networkClient.getFirmSubjectReaders(CONSTANTS.currentFirm(), this.selectedSubject, this.dateRangeManager.dateOption)\\\\n\\\\n            .then(readers => {\\\\n\\\\n              this.topReaders = readers;\\\\n              this.loadingData = false;\\\\n\\\\n            });\\\\n\\\\n        this.loadingArticles = true;\\\\n        this.networkClient.getFirmSubjectArticles(CONSTANTS.currentFirm(), this.selectedSubject, this.dateRangeManager.dateOption)\\\\n\\\\n            .then( articleDataSet => {\\\\n\\\\n              this.articleContainers = [articleDataSet];\\\\n              this.topicDataSet = [articleDataSet];\\\\n              this.loadingArticles = false;\\\\n\\\\n            });\\\\n\\\\n        this.loadingArticlesPublished = true;\\\\n        this.networkClient.getFirmOverview(CONSTANTS.currentFirm(), this.selectedSubject, this.dateRange)\\\\n\\\\n            .then( overview => {\\\\n\\\\n              this.numberOfArticlesPublished = overview.uploads;\\\\n              this.loadingArticlesPublished = false;\\\\n\\\\n            });\\\\n\\\\n        this.loadingTweeters = true;\\\\n        this.networkClient.getFirmTweeters(CONSTANTS.currentFirm(), this.selectedSubject, this.dateRange)\\\\n\\\\n            .then( tweeters => {\\\\n\\\\n              this.topTweeters = tweeters;\\\\n              this.loadingTweeters = false;\\\\n\\\\n            })\\\\n      },\\\\n\\\\n      onFirmSelected(firm) {\\\\n\\\\n      },\\\\n\\\\n      onDateChanged(option: DateRangeOption) {\\\\n        this.dateRangeManager.optionChanged(option);\\\\n      }\\\\n    }\\\\n  });\\\\n\\\\n</script>\\\\n\\\\n<style>\\\\n  .fade-enter-active, .fade-leave-active {\\\\n    transition: opacity .5s;\\\\n  }\\\\n  .fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\\\\n    opacity: 0;\\\\n  }\\\\n</style>\\\\n\\\"],\\\"sourceRoot\\\":\\\"\\\"}]);\\n\\n// exports\\n\",\"// style-loader: Adds some css to the DOM by adding a <style> tag\\n\\n// load the styles\\nvar content = require(\\\"!!../../node_modules/css-loader/index.js?sourceMap!../../node_modules/vue-loader/lib/style-compiler/index.js?{\\\\\\\"optionsId\\\\\\\":\\\\\\\"0\\\\\\\",\\\\\\\"vue\\\\\\\":true,\\\\\\\"scoped\\\\\\\":false,\\\\\\\"sourceMap\\\\\\\":true}!../../node_modules/vue-loader/lib/selector.js?type=styles&index=0!./TotalViews.vue\\\");\\nif(typeof content === 'string') content = [[module.id, content, '']];\\nif(content.locals) module.exports = content.locals;\\n// add the styles to the DOM\\nvar add = require(\\\"!../../node_modules/vue-style-loader/lib/addStylesClient.js\\\").default\\nvar update = add(\\\"0f7a95ae\\\", content, false, {});\\n// Hot Module Replacement\\nif(module.hot) {\\n // When the styles change, update the <style> tags\\n if(!content.locals) {\\n   module.hot.accept(\\\"!!../../node_modules/css-loader/index.js?sourceMap!../../node_modules/vue-loader/lib/style-compiler/index.js?{\\\\\\\"optionsId\\\\\\\":\\\\\\\"0\\\\\\\",\\\\\\\"vue\\\\\\\":true,\\\\\\\"scoped\\\\\\\":false,\\\\\\\"sourceMap\\\\\\\":true}!../../node_modules/vue-loader/lib/selector.js?type=style