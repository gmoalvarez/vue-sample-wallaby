{\\n\\n          const report = resolver.getReport(topic.name);\\n\\n          if (!report) continue;\\n\\n          if (!currentArticleViewCounts[report.name]) {\\n            currentArticleViewCounts[report.name] = article.views;\\n          }\\n\\n        }\\n\\n        // After all that, lets iterate over the current article view counts, and update the main view count dictionary\\n        // with the new view information.\\n        for (const key of Object.keys(currentArticleViewCounts)) {\\n\\n          viewCountDict[key] = viewCountDict[key] ? viewCountDict[key] + currentArticleViewCounts[key] : currentArticleViewCounts[key]\\n\\n        }\\n\\n      }\\n\\n      // Lets turn this into an array so we can sort them. So, this is a two-dimensional\\n      // array with the first element as the key, and the view count as the second element.\\n      const idViews = Object.keys(viewCountDict).map(key => {\\n        return [key, viewCountDict[key]];\\n      });\\n\\n      // 5. Ok, now lets sort by the viewcount, which is the second element in each array.\\n      idViews.sort((a, b) => {\\n        return b[1] - a[1]\\n      });\\n\\n      const reports: SubjectReport[] = [];\\n\\n      for (const idView of idViews) {\\n        const report = resolver.getReport(idView[0]);\\n        if (report) { reports.push(report)}\\n        if (reports.length === 5) { return reports }\\n      }\\n\\n      return reports;\\n    }\\n  },\\n  watch: {\\n    topSubjectReports: function(newVal, oldVal) {\\n      if (newVal && newVal.length > 0) {\\n        this.topSubjectReportCache.saveData(newVal);\\n      }\\n    }\\n  }\\n});\",\"import Vue from 'vue';\\nimport {IReader} from '../models/Reader';\\nimport {CONSTANTS} from '../entities/Constants';\\nimport { SimpleCache} from '../services/SimpleCache';\\nimport {CompanyReport} from '../models/CompanyReport';\\nimport {CompanyItemViewFactory, ItemView} from '../entities/ItemView';\\nimport {CompanyViewCounter, IViewCounter, ViewSummarizer} from '../services/ViewCounter';\\nimport {DisplayableItem} from '../services/EntitySorter';\\n\\ninterface TopCompanyData {\\n  topReaders: IReader[],\\n  companyItemViewFactory: CompanyItemViewFactory,\\n  companyViewCounter: IViewCounter,\\n  companyViewSummarizer: ViewSummarizer | null;\\n  topCompanyReportCache: SimpleCache\\n\\n}\\n\\nexport const TopCompanyMixin = Vue.extend({\\n  data(): TopCompanyData {\\n    return {\\n      topReaders: [],\\n      companyItemViewFactory: new CompanyItemViewFactory(),\\n      companyViewCounter: new CompanyViewCounter(),\\n      companyViewSummarizer: null,\\n      topCompanyReportCache: new SimpleCache(CONSTANTS.cacheKey.topCompanyReports)\\n\\n    }\\n  },\\n  created() {\\n    this.companyViewSummarizer = new ViewSummarizer(\\n        this.companyItemViewFactory,\\n        this.companyViewCounter\\n    );\\n  },\\n  computed: {\\n    companyViews(): ItemView[] {\\n      if (!this.companyViewSummarizer || !this.topReaders || this.topReaders.length === 0) { return [] }\\n\\n      return this.companyViewSummarizer.getItemViews(this.topReaders, 5);\\n    },\\n\\n    topCompanies(): any[] {\\n      console.log('Using the readers: ', this.topReaders);\\n      console.log('The company views are: ', this.companyViews);\\n      const topCompanies = this.companyViews\\n        .sort((a1,a2) => a2.views - a1.views)\\n        .map(av => av.item);\\n      console.log('The top companies are: ', topCompanies);\\n      return topCompanies\\n    },\\n\\n    topCompanyReports(): CompanyReport[] {\\n      const cachedData = this.topCompanyReportCache.getData();\\n\\n      if (cachedData) {\\n        return cachedData.map(s => CompanyReport.fromJSON(s));\\n      }\\n\\n      return this.topCompanies.slice(0,5)\\n    }\\n  },\\n\\n  watch: {\\n    topCompanyReports: function(newVal, oldVal) {\\n      if (newVal && newVal.length > 0) {\\n        this.topCompanyReportCache.saveData(newVal);\\n      }\\n    }\\n  }\\n\\n});\",\"import Vue