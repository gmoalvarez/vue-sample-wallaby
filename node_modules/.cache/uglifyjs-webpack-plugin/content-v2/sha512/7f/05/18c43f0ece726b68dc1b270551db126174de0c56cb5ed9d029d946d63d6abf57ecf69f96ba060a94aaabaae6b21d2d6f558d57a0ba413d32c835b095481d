\\n            .then(readers => {\\n\\n              this.topReaders = readers;\\n              this.loadingData = false;\\n\\n            });\\n\\n        this.loadingArticles = true;\\n        this.networkClient.getFirmSubjectArticles(CONSTANTS.currentFirm(), this.selectedSubject, this.dateRangeManager.dateOption)\\n\\n            .then( articleDataSet => {\\n\\n              this.articleContainers = [articleDataSet];\\n              this.topicDataSet = [articleDataSet];\\n              this.loadingArticles = false;\\n\\n            });\\n\\n        this.loadingArticlesPublished = true;\\n        this.networkClient.getFirmOverview(CONSTANTS.currentFirm(), this.selectedSubject, this.dateRange)\\n\\n            .then( overview => {\\n\\n              this.numberOfArticlesPublished = overview.uploads;\\n              this.loadingArticlesPublished = false;\\n\\n            });\\n\\n        this.loadingTweeters = true;\\n        this.networkClient.getFirmTweeters(CONSTANTS.currentFirm(), this.selectedSubject, this.dateRange)\\n\\n            .then( tweeters => {\\n\\n              this.topTweeters = tweeters;\\n              this.loadingTweeters = false;\\n\\n            })\\n      },\\n\\n      onFirmSelected(firm) {\\n\\n      },\\n\\n      onDateChanged(option: DateRangeOption) {\\n        this.dateRangeManager.optionChanged(option);\\n      }\\n    }\\n  });\\n\\n<\\/script>\\n\\n<style>\\n  .fade-enter-active, .fade-leave-active {\\n    transition: opacity .5s;\\n  }\\n  .fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\\n    opacity: 0;\\n  }\\n</style>\\n'],sourceRoot:\"\"}])},S4u3:function(t,e,n){var s=n(\"fWAt\");\"string\"==typeof s&&(s=[[t.i,s,\"\"]]),s.locals&&(t.exports=s.locals);(0,n(\"SZ7m\").default)(\"0f7a95ae\",s,!1,{})},SxE8:function(t,e,n){var s=n(\"v9fv\");\"string\"==typeof s&&(s=[[t.i,s,\"\"]]),s.locals&&(t.exports=s.locals);(0,n(\"SZ7m\").default)(\"645bd032\",s,!1,{})},T61v:function(t,e,n){var s=n(\"ef9i\");\"string\"==