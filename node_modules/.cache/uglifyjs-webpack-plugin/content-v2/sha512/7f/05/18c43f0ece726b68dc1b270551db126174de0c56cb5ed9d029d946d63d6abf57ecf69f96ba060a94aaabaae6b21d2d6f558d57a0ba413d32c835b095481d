 = false;\\n  private _refreshingArticleCache = false;\\n\\n  private static _instance: NetworkClient;\\n\\n  public static get Instance() {\\n    return this._instance || (this._instance = new this());\\n  }\\n\\n  private constructor(\\n    private urlConstructor: URLConstructor = new URLConstructor(),\\n    private fastCompanyReportResolver = FastCompanyReportResolver.Instance,\\n    private fastIndustryReportResolver = FastIndustryReportResolver.Instance,\\n    private fastSubjectReportResolver = FastSubjectReportResolver.Instance,\\n    private readerCache = ReaderCache.Instance,\\n    private articleCache = ArticleCache.Instance\\n  ) {\\n    this.axiosInstance = axios.create();\\n    this.axiosInstance.interceptors.request.use((config) => {\\n      console.log('Intercepting request: ', config);\\n      const token = CONSTANTS.authToken();\\n      console.log('setting the token header: ', token.token);\\n      config.headers.Authorization = 'Bearer ' + token.token;\\n      console.log('The config is now: ', config);\\n\\n      if (this.readerCache.cacheIsIncomplete && !this._refreshingReaderCache) {\\n\\n        this._refreshingReaderCache = true;\\n        setTimeout(() => {\\n          this.getAllFirmReaders();\\n        }, 1000)\\n\\n      }\\n\\n      if (this.articleCache.cacheIsIncomplete && !this._refreshingArticleCache) {\\n\\n        this._refreshingArticleCache = true;\\n        setTimeout(() => {\\n          this.getAllFirmArticles();\\n        }, 1000)\\n\\n      }\\n\\n      return config;\\n    })\\n  }\\n\\n  getCompanyReports(): Promise<CompanyReport[]> {\\n    const simpleCache = new SimpleCache(CONSTANTS.cacheKey.companyReport);\\n    const cachedData = simpleCache.getData();\\n    if (cachedData) {\\n      if (this.fastCompanyReportResolver.isEmpty) {\\n        this.fastCompanyReportResolver.populateDictionary(cachedData)\\n      }\\n      const cachedObjects: CompanyReport[] = cachedData.map(el => CompanyReport.fromJSON(el));\\n      return