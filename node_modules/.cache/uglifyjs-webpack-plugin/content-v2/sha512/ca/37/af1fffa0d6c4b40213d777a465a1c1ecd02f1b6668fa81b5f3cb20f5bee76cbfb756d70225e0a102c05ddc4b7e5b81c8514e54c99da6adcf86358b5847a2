>}\\n   */\\n  getFirmFollowers(firm: Firm, company: CompanyReport, dateRange: DateRange): Promise<Follower[]>\\n\\n  /**\\n   * Post form data for Hubspot upgrade form\\n   * @param {UpgradeFormData} formData\\n   * @returns {Promise<string>} Returns a message intended to be presented to the user.\\n   */\\n  postUpgradeFormData(formData: UpgradeFormData): Promise<string>\\n}\\n\\nexport class NetworkClient implements INetworkClient {\\n\\n  private axiosInstance: AxiosInstance;\\n\\n  constructor(\\n      private urlConstructor: URLConstructor = new URLConstructor()\\n  ) {\\n    this.axiosInstance = axios.create();\\n    // this.axiosInstance.interceptors.request.use((config) => {\\n    //   console.log('Intercepting request');\\n    //   return new Promise((resolve, reject) => {\\n    //     return this.getToken().then(() => {\\n    //       resolve(config)\\n    //     }).catch((error) => {\\n    //       reject(error)\\n    //     })\\n    //   })\\n    //\\n    // })\\n  }\\n\\n  getCompanies(): Promise<Company[]> {\\n\\n    return this.getUserData().then(() => {\\n      return this.populateReportCache().then(() => {\\n        return this.axiosInstance.get(jdsUrlStrings.endpoints.companies.base())\\n            .then(response => {\\n              const data = response.data.slice(3, 40);\\n              return data.map(el => {\\n                return Company.fromJSON(el);\\n              })\\n            })\\n      })\\n    })\\n\\n  }\\n\\n  getCompanyReports(): Promise<CompanyReport[]> {\\n    const simpleCache = new SimpleCache(CONSTANTS.cacheKey.companyReport);\\n    const cachedData = simpleCache.getData();\\n    if (cachedData) {\\n      const cachedObjects: CompanyReport[] = cachedData.map(el => CompanyReport.fromJSON(el));\\n      return Promise.resolve(cachedObjects);\\n    }\\n\\n    return this.getUserData().then(() => {\\n      return this.axiosInstance.get(jdsUrlStrings.endpoints.reports.company.base())\\n          .then((response: AxiosResponse<