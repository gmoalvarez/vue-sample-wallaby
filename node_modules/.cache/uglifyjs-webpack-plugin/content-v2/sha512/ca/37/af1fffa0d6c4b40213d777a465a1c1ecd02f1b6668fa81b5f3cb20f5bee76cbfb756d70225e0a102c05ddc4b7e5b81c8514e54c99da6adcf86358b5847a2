 companyViews');\\n      console.log(this.topReaders);\\n      console.log(this.companyViewSummarizer);\\n      if (!this.companyViewSummarizer || !this.topReaders || this.topReaders.length === 0) { return [] };\\n      console.log('companyViews: ', this.companyViewSummarizer.getItemViews(this.topReaders, 20));\\n      return this.companyViewSummarizer.getItemViews(this.topReaders);\\n    },\\n\\n    topCompanies(): CompanyReport[] {\\n      const companyNames = this.companyViews.map(s => s.item.displayTitle)\\n      console.log('company names: ', companyNames);\\n      const companies: CompanyReport[] = [];\\n      for (const name of companyNames) {\\n        const report = this.companyReportResolver.getReportByName(name);\\n        if (report) { companies.push(report) }\\n      }\\n      console.log('top companies: ', companies);\\n      return companies.slice(0,5);\\n    }\\n  }\\n\\n});\",\"\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\n\\nimport Vue from 'vue';\\nimport {SubjectReport} from '../models/SubjectReport';\\nimport {NetworkClient} from \\\"../services/NetworkClient\\\";\\nimport {Firm} from '../models/Firm';\\nimport {DateRangeOption, DateRangeUtil} from \\\"../entities/DateRangeOption\\\";\\nimport {PracticeGroupId} from '../models/PracticeGroupId';\\nimport {TopSubjectMixin} from '../mixins/TopSubjectMixin';\\nimport {TopIndustryMixin} from '../mixins/TopIndustryMixin';\\nimport {TopCompanyMixin} from '../mixins/TopCompanyMixin';\\nimport {IReader} from \\\"../models/Reader\\\";\\nimport Loading from './Loading';\\n\\nexport default Vue.extend({\\n  mixins: [\\n    TopSubjectMixin,\\n    TopIndustryMixin,\\n    TopCompanyMixin\\n  ],\\n  components: {\\n    Loading\\n  },\\n  props: [],\\n  data() {\\n    return {\\n      topReaders: <IReader[]>[],\\n      selectedFirm: new Firm('', '', '', 0),\\n      popularSubjects: [\\n        new SubjectReport('pop', 0, '', [], []),\\n      ],\