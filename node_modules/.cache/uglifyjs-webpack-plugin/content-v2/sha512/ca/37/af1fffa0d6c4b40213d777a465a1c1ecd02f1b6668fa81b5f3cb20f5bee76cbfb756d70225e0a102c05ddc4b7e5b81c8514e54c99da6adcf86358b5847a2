;\\\\n  import { TopTopicMixin } from \\\\\\\"../mixins/TopTopicMixin\\\\\\\";\\\\n  import { TopArticleMixin } from \\\\\\\"../mixins/TopArticleMixin\\\\\\\";\\\\n  import { TopAuthorMixin} from \\\\\\\"../mixins/TopAuthorMixin\\\\\\\";\\\\n  import { TopIndustryMixin } from \\\\\\\"../mixins/TopIndustryMixin\\\\\\\";\\\\n  import { TopTweeterMixin } from \\\\\\\"../mixins/TopTweeterMixin\\\\\\\";\\\\n  import { ModalMixin } from '../mixins/ModalMixin';\\\\n\\\\n\\\\n  export default Vue.extend({\\\\n    mixins: [\\\\n\\\\n      TopSubjectMixin,\\\\n      TopTopicMixin,\\\\n      TopArticleMixin,\\\\n      TopAuthorMixin,\\\\n      TopIndustryMixin,\\\\n      TopTweeterMixin,\\\\n      ModalMixin\\\\n\\\\n    ],\\\\n\\\\n    components: {\\\\n\\\\n      TopItems,\\\\n      NumberSummary,\\\\n      TotalViews,\\\\n      Loading,\\\\n      TopicSummary,\\\\n      SubjectSummary,\\\\n      IndustrySummary,\\\\n      FirmSelect,\\\\n      TopReaders,\\\\n      SubjectHeader,\\\\n      TopTweeters,\\\\n      Upgrade\\\\n\\\\n    },\\\\n\\\\n    props: ['id'],\\\\n\\\\n    data() {\\\\n      return {\\\\n        showDropdown: false,\\\\n        selectedFirm: new Firm('', '', '', 0),\\\\n        subjects: <SubjectReport[]>[],\\\\n        selectedSubject: new SubjectReport('', 0, '', [], []),\\\\n        topReaders: <IReader[]>[],\\\\n        topTweeters: <Tweeter[]>[],\\\\n        loadingData: false,\\\\n        loadingSubjects: false,\\\\n        loadingArticles: false,\\\\n        loadingArticlesPublished: false,\\\\n        loadingTweeters: false,\\\\n        numberOfArticlesPublished: 0,\\\\n        articleSummaryFactory: new ArticleSummaryFactory(),\\\\n        topicItemViewFactory: new TopicItemViewFactory(),\\\\n        topicViewCounter: new TopicViewCounter(),\\\\n        networkClient: new NetworkClient(),\\\\n        dateRangeManager: new DateRangeUtil(),\\\\n        snsPublisher: new SNSMessagePublisher(),\\\\n        currentUser: new CurrentUser(),\\\\n\\\\n        //Data for Mixins\\\\n        articleContainers: <ArticleContaining[]>[],\\\\n        topicDataSet: <ArticleViewDataSet[]>[]\\\\n      }\\\\n    },\\\\n\\\\n    created() {\\\\n\\\\n      this.loadingSubjects = true;\\\\n      this.loadingArticles = true;\\\\n      this.loadSubjectReports()\\\\n\\\\n    },\\\\n    watch: {\\\\n\\\\n      selectedSubject: function(newVal, oldVal) {\\\\n        this.snsPublisher.publishSearchEvent(this.selectedSubject.id);\\\\n\\\\n        this.getData();\\\\n\\\\n      },\\\\n\\\\n      selectedFirm: function() {\\\\n\\\\n        this.getData();\\\\n\\\\n      },\\\\n\\\\n      dateRange: function() {\\\\n\\\\n        this.getData();\\\\n\\\\n      },\\\\n\\\\n      id: function() {\\\\n        if (!this.subjects || this.subjects.length <= 0 || !this.id) { return }\\\\n\\\\n        this.setSelectedSubjectFromId();\\\\n\\\\n      }\\\\n    },\\\\n    computed: {\\\\n\\\\n      dateRange(): DateRange {\\\\n        return this.dateRangeManager.dateRange\\\\n      },\\\\n\\\\n      dataIsIncomplete(): boolean {\\\\n        return this.loadingData ||\\\\n            !this.selectedSubject ||\\\\n            !this.selectedSubject.topicIds ||\\\\n            !this.selectedSubject.subjectIds ||\\\\n            (this.selectedSubject.topicIds.length <= 0 && this.selectedSubject.subjectIds.length <= 0)\\\\n      }\\\\n    },\\\\n    methods: {\\\\n\\\\n      setSelectedSubjectFromId() {\\\\n        for (const subject of this.subjects) {\\\\n\\\\n          // this.id from route is a string, while subject.id is a number, so we use == here\\\\n          if (subject.id == this.id) {\\\\n\\\\n            this.selectedSubject = subject;\\\\n            this.getData();\\\\n            return\\\\n\\\\n          }\\\\n\\\\n        }\\\\n      },\\\\n\\\\n      loadSubjectReports() {\\\\n\\\\n        this.networkClient.getSubjectReports()\\\\n            .then(subjects => {\\\\n\\\\n              this.subjects = subjects;\\\\n              this.loadingSubjects = false;\\\\n              this.setSelectedSubjectFromId();\\\\n\\\\n            });\\\\n\\\\n      },\\\\n\\\\n      getPGIds(): Promise<PracticeGroupId> {\\\\n\\\\n        return this.networkClient.getFirmPracticeGroupIds()\\\\n\\\\n            .then( ids => {\\\\n\\\\n              this.selectedFirm.id = ids.parent;\\\\n              this.selectedFirm.practiceGroupIds = ids.children;\\\\n              return ids;\\\\n\\\\n            })\\\\n      },\\\\n\\\\n      getData() {\\\\n\\\\n        if (this.dataIsIncomplete) { return }\\\\n\\\\n        // Lets make sure we have both a company and a subject\\\\n        if (!this.selectedFirm || !this.selectedSubject || !this.dateRange) { return }\\\\n\\\\n        //Lets make sure we have all of the practice group ids for this firm.\\\\n        if (this.selectedFirm.id === 0 || this.selectedFirm.allRequestIds.length === 0) {\\\\n          this.loadingData = true;\\\\n          this.getPGIds()\\\\n              .then(() => {\\\\n                this.loadingData = false;\\\\n                this.getData();\\\\n              });\\\\n          return;\\\\n        }\\\\n\\\\n        this.loadingData = true;\\\\n\\\\n        this.networkClient.getFirmSubjectReaders(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n\\\\n            .then(readers => {\\\\n\\\\n              this.topReaders = readers;\\\\n              this.loadingData = false;\\\\n\\\\n            });\\\\n\\\\n        this.loadingArticles = true;\\\\n        this.networkClient.getFirmSubjectArticles(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n\\\\n            .then( articleDataSet => {\\\\n\\\\n              this.articleContainers = [articleDataSet];\\\\n              this.topicDataSet = [articleDataSet];\\\\n              this.loadingArticles = false;\\\\n\\\\n            });\\\\n\\\\n        this.loadingArticlesPublished = true;\\\\n        this.networkClient.getFirmOverview(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n\\\\n            .then( overview => {\\\\n\\\\n              this.numberOfArticlesPublished = overview.uploads;\\\\n              this.loadingArticlesPublished = false;\\\\n\\\\n            });\\\\n\\\\n        this.loadingTweeters = true;\\\\n        this.networkClient.getFirmTweeters(this.selectedFirm, this.selectedSubject, this.dateRange)\\\\n\\\\n            .then( tweeters => {\\\\n\\\\n              this.topTweeters = tweeters;\\\\n              this.loadingTweeters = false;\\\\n\\\\n            })\\\\n      },\\\\n\\\\n      onFirmSelected(firm) {\\\\n        this.selectedFirm = firm;\\\\n      },\\\\n\\\\n      onDateChanged(option: DateRangeOption) {\\\\n        this.dateRangeManager.optionChanged(option);\\\\n      }\\\\n    }\\\\n  });\\\\n\\\\n</script>\\\\n\\\\n<style>\\\\n  .fade-enter-active, .fade-leave-active {\\\\n    transition: opacity .5s;\\\\n  }\\\\n  .fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\\\\n    opacity: 0;\\\\n  }\\\\n</style>\\\\n\\\"],\\\"sourceRoot\\\":\\\"\\\"}]);\\n\\n// exports\\n\",\"// style-loader: Adds some css to the DOM by adding a <style> tag\\n\\n// load the styles\\nvar content = require(\\\"!!../../node_modules/css-loader/index.js?sourceMap!../../node_modules/vue-loader/lib/style-compiler/index.js?{\\\\\\\"optionsId\\\\\\\":\\\\\\\"0\\\\\\\",\\\\\\\"vue\\\\\\\":true,\\\\\\\"scoped\\\\\\\":false,\\\\\\\"sourceMap\\\\\\\":true}!../../node_modules/vue-loader/lib/selector.js?type=styles&index=0!./TotalViews.vue\\\");\\nif(typeof content === 'string') content = [[module.id, content, '']];\\nif(content.locals) module.exports = content.locals;\\n// add the styles to the DOM\\nvar add = require(\\\"!../../node_modules/vue-style-loader/lib/addStylesClient.js\\\").default\\nvar update = add(\\\"0f7a95ae\\\", content, false, {});\\n// Hot Module Replacement\\nif(module.hot) {\\n // When the styles change, update the <style> tags\\n if(!content.locals) {\\n   module.hot.accept(\\\"!!../