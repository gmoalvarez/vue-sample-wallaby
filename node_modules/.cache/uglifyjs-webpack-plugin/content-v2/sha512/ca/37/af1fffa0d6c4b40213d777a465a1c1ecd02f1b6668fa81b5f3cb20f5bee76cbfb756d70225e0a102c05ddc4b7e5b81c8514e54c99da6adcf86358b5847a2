ange} from \\\\\\\"../entities/DateRange\\\\\\\";\\\\n  import {PracticeGroupId} from '../models/PracticeGroupId';\\\\n  import {Follower} from '../models/Follower';\\\\n  import {ArticleViewDataSet} from '../entities/union-types';\\\\n\\\\n  //Mixins\\\\n  import {TopArticleMixin} from '../mixins/TopArticleMixin';\\\\n  import { TopTopicMixin } from \\\\\\\"../mixins/TopTopicMixin\\\\\\\";\\\\n  import { TopAuthorMixin } from \\\\\\\"../mixins/TopAuthorMixin\\\\\\\";\\\\n  import { TopSubjectMixin } from '../mixins/TopSubjectMixin';\\\\n  import { ModalMixin } from '../mixins/ModalMixin';\\\\n\\\\n  // Services\\\\n  import { SNSMessagePublisher} from '../services/SNSMessagePublisher';\\\\n  import { CurrentUser} from '../services/CurrentUser';\\\\n\\\\n  export default Vue.extend({\\\\n    mixins: [\\\\n      TopArticleMixin,\\\\n      TopTopicMixin,\\\\n      TopAuthorMixin,\\\\n      TopSubjectMixin,\\\\n        ModalMixin\\\\n    ],\\\\n    components: {\\\\n      FirmSelect,\\\\n      CompanySelect,\\\\n      DateRangeSelector,\\\\n      Loading,\\\\n      NumberSummary,\\\\n      TotalViews,\\\\n      TopItems,\\\\n      TopicSummary,\\\\n      SubjectSummary,\\\\n      TopReaders,\\\\n      Upgrade,\\\\n      CompanyHeader\\\\n    },\\\\n    props: ['id'],\\\\n\\\\n    data() {\\\\n      return {\\\\n        selectedFirm: new Firm('', '', '', 0),\\\\n        companies: <CompanyReport[]>[],\\\\n        selectedCompany: new CompanyReport('', 0, '', []),\\\\n        networkClient: new NetworkClient(),\\\\n        loading: false,\\\\n        loadingCompanies: false,\\\\n        loadingFollowers: false,\\\\n        dateRangeManager: new DateRangeUtil(),\\\\n        topReaders: <IReader[]>[],\\\\n        topFollowers: <Follower[]>[],\\\\n        snsPublisher: new SNSMessagePublisher(),\\\\n        currentUser: new CurrentUser(),\\\\n\\\\n        //Data for Mixins\\\\n        articleContainers: <ArticleContaining[]>[],\\\\n        topicDataSet: <ArticleViewDataSet[]>[]\\\\n      }\\\\n    },\\\\n    created() {\\\\n      this.loadingCompanies = true;\\\\n      this.networkClient.getCompanyReports()\\\\n          .then(companies => {\\\\n            this.companies = companies;\\\\n            this.loadingCompanies = false;\\\\n            for (const company of this.companies) {\\\\n              // this.id from route is a string, while subject.id is a number, so we use == here\\\\n              if (company.id == this.id) {\\\\n                this.selectedCompany = company;\\\\n                this.getData();\\\\n                return\\\\n              }\\\\n            }\\\\n          })\\\\n    },\\\\n    computed: {\\\\n      dateRange(): DateRange {\\\\n        return this.dateRangeManager.dateRange\\\\n      },\\\\n\\\\n      dataIsIncomplete(): boolean {\\\\n        return this.loading ||\\\\n            !this.selectedCompany ||\\\\n            !this.selectedCompany.companyIds ||\\\\n            this.selectedCompany.companyIds.length <= 0\\\\n      }\\\\n    },\\\\n    methods: {\\\\n      getPGIds(): Promise<PracticeGroupId> {\\\\n        return this.networkClient.getFirmPracticeGroupIds()\\\\n            .then( ids => {\\\\n              this.selectedFirm.id = ids.parent;\\\\n              this.selectedFirm.practiceGroupIds = ids.children;\\\\n              return ids;\\\\n            })\\\\n      },\\\\n      onFirmSelected(firm) {\\\\n        this.selectedFirm = firm;\\\\n      },\\\\n      onCompanySelected(company: CompanyReport) {\\\\n        this.$router.push(`/company/${company.id}`);\\\\n      },\\\\n      onDateChanged(option: DateRangeOption) {\\\\n        this.dateRangeManager.optionChanged(option);\\\\n      },\\\\n      getData() {\\\\n        if (this.dataIsIncomplete) { return }\\\\n\\\\n        // Lets make sure we have both a company and a subject\\\\n        if (!this.selectedFirm || !this.selectedCompany || !this.dateRange) { return }\\\\n\\\\n        //Lets make sure we have all of the practice group ids for this firm.\\\\n        if (this.selectedFirm.id === 0 || this.selectedFirm.allRequestIds.length === 0) {\\\\n          this.loading = true;\\\\n          this.getPGIds()\\\\n              .then(() => {\\\\n                this.loading = false;\\\\n                this.getData();\\\\n              });\\\\n          return;\\\\n        }\\\\n\\\\n        this.loading = true;\\\\n\\\\n        this.networkClient\\\\n            .getFirmCompanyReaders(this.selectedFirm, this.selectedCompany, this.dateRange)\\\\n            .then(readers => {\\\\n\\\\n              this.topReaders = readers;\\\\n              this.loading = false;\\\\n\\\\n            });\\\\n\\\\n        this.loadingFollowers = true;\\\\n        this.networkClient\\\\n            .getFirmFollowers(this.selectedFirm, this.selectedCompany, this.dateRange)\\\\n            .then( followers => {\\\\n\\\\n              this.topFollowers = followers.sort((a,b) => {\\\\n                return b.dateAdded.valueOf() - a.dateAdded.valueOf();\\\\n              });\\\\n              this.loadingFollowers = false;\\\\n\\\\n            })\\\\n      }\\\\n    },\\\\n    watch: {\\\\n      selectedCompany: function(newVal, oldVal) {\\\\n        this.snsPublisher.publishSearchEvent(this.selectedCompany.id);\\\\n\\\\n        this.getData();\\\\n      },\\\\n      selectedFirm: function() {\\\\n        this.getData();\\\\n      },\\\\n      id: function() {\\\\n        if (!this.companies || this.companies.length <= 0 || !this.id) { return }\\\\n\\\\n        for (const company of this.companies) {\\\\n          if (company.id == this.id) {\\\\n            this.selectedCompany = company;\\\\n            this.getData();\\\\n            return\\\\n          }\\\\n        }\\\\n      },\\\\n      topReaders: function() {\\\\n        this.articleContainers = this.topReaders;\\\\n        this.topicDataSet = this.topReaders;\\\\n      },\\\\n      dateRange: function() {\\\\n        this.getData();\\\\n      },\\\\n    }\\\\n  });\\\\n\\\\n</script>\\\\n\\\\n<style>\\\\n    .fade-enter-active, .fade-leave-active {\\\\n        transition: opacity .5s;\\\\n    }\\\\n    .fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\\\\n        opacity: 0;\\\\n    }\\\\n</style>\\\\n\\\"],\\\"sourceRoot\\\":\\\"\\\"}]);\\n\\n// exports\\n\",\"import {Pie} from 'vue-chartjs';\\n\\nexport default {\\n  extends: Pie,\\n  props: ['options', 'data'],\\n  mounted () {\\n    this.render()\\n  },\\n  data: () => ({\\n    htmlLegend: null\\n  }),\\n  watch: {\\n    options: function() {\\n      this.render()\\n    },\\n    data: function() {\\n      console.log('data changed', this.data);\\n      this.render()\\n    }\\n  },\\n  methods: {\\n    render: function() {\\n      this.renderChart(this.data, this.options);\\n      this.htmlLegend = this.generateLegend();\\n    }\\n  }\\n};\",\"// style-loader: Adds some css to the DOM by adding a <style> tag\\n\\n// load the styles\\nvar content = require(\\\"!!../../node_modules/css-loader/index.js?sourceMap!../../node_modules/vue-loader/lib/style-compiler/index.js?{\\\\\\\"optionsId\\\\\\\":\\\\\\\"0\\\\\\\",\\\\\\\"vue\\\\\\\":true,\\\\\\\"scoped\\\\\\\":false,\\\\\\\"sourceMap\\\\\\\":true}!../../node_modules/vue-loader/lib/selector.js?type=styles&index=0!./SubjectSummary.vue\\\");\\nif(typeof content === 'string') content = [[module.id, content, '']];\\nif(content.locals) module.exports = content.locals;\\n// add the styles to the DOM\\nvar add = require(\\\"!../../node_modules/vue-style-loader/lib/addStylesClient.js\\\").default\\nvar update = add(\\\"1b1a9f4e\\\", content, false, {});\\n// Hot Module Replacement\\nif(module.hot) {\\n // When the styles change, update the <style> tags\\n if(!content.locals) {\\n   module.hot.accept(\\\"!!../../node_modules/css-loader/index.js?sourceMap!../../node_modules/vue-loader/lib/style-compiler/index.js?{\\\\\\\"optionsId\\\\\\\":\\\\\\\"0\\\\\\\",\\\\\\\"vue\\\\\\\":true,\\\\\\\"scoped\\\\\\