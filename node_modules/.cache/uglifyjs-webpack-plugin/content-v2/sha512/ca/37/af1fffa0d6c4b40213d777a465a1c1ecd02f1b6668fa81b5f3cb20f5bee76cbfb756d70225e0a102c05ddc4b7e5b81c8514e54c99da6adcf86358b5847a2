/* Use the readers array, and extract the top articles. */\\n  getSummary(collection: ArticleContaining[], top?: number): Summarizable {\\n    // Since the articles are embedded inside the readers array, we need to do a bit of work to\\n    // get the top articles.\\n    // 1. Lets set up dictionaries to hold the view counts and the article guids with the view counts\\n    const viewCount: any = {};\\n    const itemDictionary: any = {};\\n\\n    // 2. Lets get the two-dimensional array of articles from the readers\\n    const articlesArray = this.getArticlesArray(collection);\\n\\n    // 3. Now lets iterate over those articles and populate the viewCount dictionary\\n    // We end up with a dictionary that holds the guids of the articles as the key, and\\n    // the view counts as the value\\n    for (const articles of articlesArray) {\\n      for (const article of articles) {\\n        const id = article.identifier;\\n        if (viewCount[`${id}`]) {\\n          viewCount[`${id}`] += article.views;\\n        } else {\\n          viewCount[`${id}`] = article.views;\\n          itemDictionary[`${id}`] = article;\\n        }\\n      }\\n    }\\n\\n    // 4. Lets turn this into an array so we can sort them. So, this is a two-dimensional\\n    // array with the first element as the key, and the view count as the second element.\\n    const items = Object.keys(viewCount).map(key => {\\n      return [key, viewCount[key]];\\n    });\\n\\n    // 5. Ok, now lets sort by the viewcount, which is the second element in each array.\\n    console.log('sorting by viewcount: ', items);\\n    items.sort( (a, b) => {\\n      return b[1] - a[1]\\n    });\\n    console.log('after sorting:', items);\\n    // 6. We might only want the top articles. If so, lets slice the array\\n    // The id is in index [0]. The viewcount is in index[1]\\n    let articleIdViewCount: any[][] = [[]];\\n    if (top) {\\n      articleIdViewCount = items.slice(0,top);\\n    }\\n    console.log('top articles: ', articleIdViewCount);\\n    // Done. We can get the articles themselves back from the original dictionary using\\n    // the file guid as the key.\\\\\\n\\n    const articles = articleIdViewCount\\n        .map(idViewCount => {\\n          const article = itemDictionary[`${idViewCount[0]}`];\\n          article.views = idViewCount[1];\\n          return article;\\n        });\\n\\n\\n    console.log('articles: ', articles);\\n    const totalArticleCount = items.length;\\n    console.log('total article count: ', totalArticleCount);\\n    const totalViewCount = items.reduce((acc,cur) => { return acc + cur[1] }, 0);\\n    console.log('totalViewCount: ', totalViewCount);\\n    return this._factory.createSummary(totalViewCount, totalArticleCount, articles)\\n    //return new ArticleSummary(totalVievwCount, totalArticleCount, articles);\\n  }\\n\\n  private getArticlesArray(collection: ArticleContaining[]) {\\n    return collection\\n        .map(item => {\\n          return item.articles;\\n        });\\n  }\\n\\n}\",\"import {Tweet, TweetDTO} from './Tweet';\\nimport {Article} from './Article';\\nimport {ArticleContaining, EntitySorter} from '../services/EntitySorter';\\n\\nexport class Tweeter implements ArticleContaining {\\n\\n    constructor(\\n        public name: string,\\n        public description: string,\\n        public tweetCount: number,\\n        public tweets: Tweet[],\\n        public photoUrl: string,\\n        public notable: boolean,\\n        public screenName: string,\\n        public totalFollowers: number = 0\\n    ) {}\\n\\n    get articles(): Article[] {\\n        const articles: Article[] = [];\\n\\n        for (const tweet of this.tweets) {\\n            if (tweet.articles && tweet.articles.length > 0) {\\n              articles.push(tweet.articles[0]);\\n            }\\n        }\\n\\n        return articles;\\n    }\\n\\n    get profileUrl(): string {\\n        return `https://twitter.com/${this.screenName}`\\n    }\\n\\n    get topArticleN